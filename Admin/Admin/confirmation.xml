<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame">
		<property name="title" value="Confirmation" translatable="yes" />
		<widget class="SwatForm" name="confirmation_form">
			<widget class="SwatContentBlock" name="confirmation_message" />
			<widget class="SwatFormField">
				<property name="class" value="swat-form-footer" />
				<widget class="SwatButton" name="yes_button">
					<property name="title" value="Yes" translatable="yes" />
				</widget>
				<widget class="SwatButton" name="no_button">
					<property name="title" value="No" translatable="yes" />
				</widget>
			</widget>
		</widget>
	</widget>
</swat-interface>