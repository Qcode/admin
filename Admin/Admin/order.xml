<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="order_frame">
		<widget class="SwatForm" id="order_form">
			<widget class="SwatFormFooterField">
				<property name="title" value="Order Options" translatable="yes" />
				<widget class="SwatRadioList" id="options" />
			</widget>
			<widget class="SwatFormFooterField">
				<property name="title" value="Custom Order" translatable="yes" />
				<widget class="SwatChangeOrder" id="order" />
			</widget>
			<widget class="SwatFormFooterField">
				<widget class="SwatButton" id="submit_button" />
			</widget>
		</widget>
	</widget>
</swat-interface>
