<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatFrame" id="index_frame">
		<property name="title" translatable="yes">Admin User Login History</property>
		<widget class="SwatForm" id="index_form">
			<widget class="SwatTableView" id="index_view">
				<object class="AdminTableViewOrderableColumn" id="username">
					<property name="title" translatable="yes">Username</property>
					<object class="SwatLinkCellRenderer">
						<property name="text" type="data">username</property>
						<property name="link">AdminUsers/Details?id=%s</property>
						<property name="value" type="data">usernum</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="logindate">
					<property name="title" translatable="yes">Login Time</property>
					<object class="SwatDateCellRenderer">
						<property name="date" type="data">logindate</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="loginagent">
					<property name="title" translatable="yes">Agent</property>
					<object class="SwatTextCellRenderer">
						<property name="text" type="data">loginagent</property>
					</object>
				</object>
				<object class="AdminTableViewOrderableColumn" id="remoteip">
					<property name="title" translatable="yes">Remote IP</property>
					<object class="SwatTextCellRenderer">
						<property name="text" type="data">remoteip</property>
					</object>
				</object>
			</widget>
		</widget>
		<widget class="AdminPagination" id="pager" />
	</widget>
</swatml>
