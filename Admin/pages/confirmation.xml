<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatFrame">
		<property name="title" translatable="yes">Confirmation</property>
		<widget class="SwatForm" id="confirmation_form">
			<widget class="SwatContentBlock" id="confirmation_message" />
			<widget class="SwatFormFooterField">
				<widget class="SwatButton" id="no_button">
					<property name="title" translatable="yes">No</property>
				</widget>
				<widget class="SwatButton" id="yes_button">
					<property name="title" translatable="yes">Yes</property>
				</widget>
			</widget>
		</widget>
	</widget>
</swatml>
